{
  "compilerOptions": {
    "target": "ES6",
    "moduleResolution": "node",
    "module": "commonjs",
    "strict": false,
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "Components/*": [
        "src/Components/*"
      ],
      "Images/": [
        "src/Images/*"
      ],
      "Context/*": [
        "src/Context/*"
      ],
      "Page/*": [
        "src/Page/*"
      ],
    }
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "./node_modules/",
    "./dist"
  ]
}